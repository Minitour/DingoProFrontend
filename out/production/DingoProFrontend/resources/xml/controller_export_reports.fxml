<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDatePicker?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import ui.UIView?>
<UIView fx:id="root" xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        stylesheets="@../../ui/res/css/styles.css,@../css/style.css">
    <VBox spacing="16">
        <padding><Insets bottom="8" left="8" right="8" top="8"/></padding>
        <JFXDatePicker fx:id="fromDate" promptText="From Date" overLay="false" defaultColor="#40739e"/>
        <JFXDatePicker fx:id="toDate" promptText="To Date" overLay="false" defaultColor="#40739e"/>
        <AnchorPane fx:id="jasper_container" VBox.vgrow="ALWAYS"/>
    </VBox>

</UIView>
